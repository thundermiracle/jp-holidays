{
  "name": "jp-holidays",
  "version": "0.1.0",
  "packageManager": "pnpm@8.7.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:w run --coverage",
    "test:w": "vitest --dir __test__",
    "test:e2e": "dotenv -e .env.e2e.local -- vitest --dir e2e",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "batch:ts": "tsx batch/index.ts",
    "pack:batch": "ncc build batch/index.ts -o dist --source-map --license licenses.txt",
    "batch": "node dist/index.js"
  },
  "dependencies": {
    "dayjs": "^1.11.9",
    "next": "^13.4.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@edge-runtime/vm": "3.1.0",
    "@vercel/ncc": "^0.36.1",
    "@types/node": "^18.17.12",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@types/supertest": "^2.0.12",
    "@web-configs/eslint-plugin": "^0.5.2",
    "@web-configs/prettier": "^0.1.2",
    "@web-configs/typescript": "^0.2.2",
    "axios": "^1.5.0",
    "c8": "^8.0.1",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "eslint-config-next": "^13.4.19",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "tsx": "^3.12.7",
    "typescript": "^5.2.2",
    "vitest": "^0.20.3"
  },
  "prettier": "@web-configs/prettier"
}
