{
  "name": "jp-holidays",
  "version": "0.1.0",
  "packageManager": "pnpm@9.15.9",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:w run --coverage",
    "test:w": "vitest --dir __test__",
    "test:e2e": "dotenv -e .env.e2e.local -- vitest --dir e2e",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "batch:ts": "tsx batch/index.ts",
    "pack:batch": "ncc build batch/index.ts -o dist --source-map --license licenses.txt",
    "batch": "node dist/index.js"
  },
  "dependencies": {
    "dayjs": "^1.11.13",
    "next": "^15.3.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@edge-runtime/vm": "5.0.0",
    "@vercel/ncc": "^0.38.3",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.0",
    "@web-configs/eslint-plugin": "^0.5.2",
    "@web-configs/prettier": "^0.1.2",
    "@web-configs/typescript": "^0.2.2",
    "axios": "^1.9.0",
    "c8": "^10.1.3",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "prettier": "^3.5.3",
    "supertest": "^7.1.1",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "vitest": "^3.2.0"
  },
  "prettier": "@web-configs/prettier"
}
