{
  "name": "jp-holidays",
  "version": "0.1.0",
  "packageManager": "pnpm@8.15.5",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "pnpm test:w run --coverage",
    "test:w": "vitest --dir __test__",
    "test:e2e": "dotenv -e .env.e2e.local -- vitest --dir e2e",
    "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "batch:ts": "tsx batch/index.ts",
    "pack:batch": "ncc build batch/index.ts -o dist --source-map --license licenses.txt",
    "batch": "node dist/index.js"
  },
  "dependencies": {
    "dayjs": "^1.11.10",
    "next": "^13.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@edge-runtime/vm": "3.2.0",
    "@vercel/ncc": "^0.38.1",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "^1.4.0",
    "@web-configs/eslint-plugin": "^0.5.2",
    "@web-configs/prettier": "^0.1.2",
    "@web-configs/typescript": "^0.2.2",
    "axios": "^1.6.8",
    "c8": "^8.0.1",
    "dotenv-cli": "^7.4.1",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.1.4",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vitest": "^1.4.0"
  },
  "prettier": "@web-configs/prettier"
}
